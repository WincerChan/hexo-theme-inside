<!-- <script src="https://cdn.jsdelivr.net/gh/wincerchan/hexo-theme-inside@1.9.9/source/vendor.9bc655.js" integrity="sha256-9L6H2BRROis63kEDyU6Pqn70sUvFNT5xS8i8Oks49jQ=" crossorigin="anonymous"></script> -->
<script src="/vendor.9bc655.js"></script>
<!-- <%- load_js() %></script> -->
</body>
