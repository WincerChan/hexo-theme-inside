<!doctype html>
<html lang="<%= config.language || 'en' %>">
<head>
<meta charset="utf-8">
<title><%= title %></title>
<base href="<%= config.root %>">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=2">
<link rel="icon" type="image/x-icon" href="<%= url_for(theme.favicon) %>">
<% if (theme.manifest.name) {
%><link rel="manifest" href="<%= url_for('manifest.json') %>">
<% }
%>
<meta name="format-detection" content="telephone=no">
<meta name="theme-color" content="<%= theme.manifest.theme_color %>">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="translucent">
<meta name="apple-mobile-web-app-title" content="<%= config.title %>">
<meta name="mobile-web-app-capable" content="yes">
<meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
<meta name="application-name" content="<%= config.title %>">
<meta name="msapplication-starturl" content="<%= config.url %>">
<meta name="msapplication-navbutton-color" content="<%= theme.manifest.theme_color %>">
<meta name="author" content="<%= config.author %>">
<meta name="HandheldFriendly" content="True">
<meta name="MobileOptimized" content="480">
<% if( page.slug ) { %>
<link rel="canonical" href="<%- config.url + url_for(path).slice(0, -10) %>">
<meta name="keywords" content="<% if(page.tags && page.tags.each) { page.tags.each(function(tag) { %><%- tag.name + ',' %><% })} %>Wincer, Wincer Blog, Blog">
<meta name="description" content="<% if(page.description) { %><%= page.description %><% } else if(page.excerpt){ %><%= strip_html(page.excerpt).replace(/^s*/, '').replace(/s*$/, '') %><% } else if (config.description){ %><%= config.description %><% } %>">
<meta property="og:url" content="<%- config.url + url_for(path).slice(0, -10) %>">
<meta property="og:type" content="blog">
<meta property="og:title" content="<%- page.title %>">
<meta property="og:image" content="<%= theme.profile.avatar %>">
<meta property="og:description" content="<% if(page.description) { %><%= page.description %><% } else if(page.excerpt){ %><%= strip_html(page.excerpt).replace(/^s*/, '').replace(/s*$/, '') %><% } else if (config.description){ %><%= config.description %><% } %>">
<% if(page.tags && page.tags.each) { %>
<% page.tags.each(function(tag) { %>
<meta property="og:article:tag" content="<%= tag.name %>">
<% }) %>
<% } %>
<meta property="article:published_time" content="<%= page.date %>">
<meta property="article:modified_time" content="<%= page.updated %>">
<meta name="twitter:title" content="<%- page.title %>">
<meta name="twitter:description" content="<% if(page.description) { %><%= page.description %><% } else if(page.excerpt){ %><%= strip_html(page.excerpt).replace(/^s*/, '').replace(/s*$/, '') %><% } else if (config.description){ %><%= config.description %><% } %>">
<meta name="twitter:image" content="<%= theme.profile.avatar %>">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:url" content="<%- config.url + url_for(path).slice(0, -10) %>">
<% } else { %>
<link rel="canonical" href="<%= config.url + url_for(path).slice(0, -10) %>">
<meta name="keywords" content="Wincer, Wincer Blog, Blog">
<meta name="description" content="<%= config.description %>">
<meta property="og:url" content="<%= config.url %>">
<meta property="og:type" content="blog">
<meta property="og:title" content="<%= config.title %>">
<meta property="og:image" content="<%= theme.profile.avatar %>">
<meta property="og:description" content="<%= config.description %>">
<meta name="twitter:title" content="<%= config.title %>">
<meta name="twitter:description" content="<%= config.description %>">
<meta name="twitter:image" content="<%= theme.profile.avatar %>">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:url" content="<%= config.url %>">
<% } %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/wincerchan/hexo-theme-inside@1.9.4/source/styles.133ef57.css" integrity="sha256-9Sqfq/MN5x4Jm1uERkkNEI05kd73Dz0v7sVHMzkcfFY=" crossorigin="anonymous">
<% if (config.feed && config.feed.path) {%>
<link rel="alternate" type="application/rss+xml" title="<%= config.title %>" href="<%= url_for(config.feed.path) %>">
<%}
%><!-- <%- load_css() %> -->
<% if (theme.head.structured_data === true ){ %>
    <%- partial('structured-data') %>
<% } %>
<!-- <%- css('theme.' + theme.themeHash)%> -->
<!-- <%- js('config.' + theme.hash) %> -->
<!-- <%- load_css() %> -->
<script>window['prefix']='https://gitee.com/whisk/blog_api/raw/master'</script>
<script>window.__inside__=<%- global.config %></script>
</head>
<body>
<noscript>You need to enable JavaScript to see this page.</noscript>
<is-root></is-root>
<%- ga() %>
